{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "sequelize": "^6.21.2"
  },
  "devDependencies": {
    "sequelize-cli": "^6.4.1",
    "sequelize-mig": "^3.1.3"
  },
  "scripts": {
    "db-init": "sequelize-cli db:create --env production",
    "migration-create": "sequelize-mig migration:make -n mig",
    "migrate": "sequelize-cli db:migrate --env production",
    "build": "yarn db-init; yarn migration-create; yarn migrate",

    "start": "node index.js"
  }
}
